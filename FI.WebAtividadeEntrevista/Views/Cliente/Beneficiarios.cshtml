<div id="modalBeneficiarios" class="modal fade" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <p class="modal-title fs-5">Beneficiários</p>
            </div>
            <div class="modal-body">
                <div class="row">
                    <form id="formBeneficiario" method="post">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label for="CPFBeneficiario">CPF:</label>
                                <input required="required" type="text" class="form-control" id="CPFBeneficiario" name="CPFBeneficiario" placeholder="Ex.: 010.011.111-00" maxlength="14" oninput="MascaraCPF(this)">
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="NomeBeneficiario">Nome:</label>
                                <input required="required" type="text" class="form-control" id="NomeBeneficiario" name="NomeBeneficiario" placeholder="Ex.: Maria" maxlength="50">
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group" style="margin-top:40%">
                                <button type="button" id="bntBeneficiario" class="btn btn-success" onclick="AddItem()">Incluir</button>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <table id="tableBeneficiarios" class="table">
                            <thead>
                                <tr>
                                    <th style="width:25%" scope="col">CPF</th>
                                    <th style="width:45%" scope="col">Nome</th>
                                    <th style="width:30%" scope="col">Ações</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>